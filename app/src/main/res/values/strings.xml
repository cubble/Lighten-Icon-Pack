<resources>
    <string name="app_name">Lighten Theme</string>

    <!-- Basic theme information -->
    <string name="theme_title">Lighten Icon Pack</string>
    <string name="developer_name">webh</string>
    <!-- this_icon (optional) link will appear at the end of the theme description -->
    <string name="developer_link">https://play.google.com/store/apps/developer?id=webh</string>
    <string name="theme_description">Lighten is a small icon theme which applies a white circle mask on all app icons. It do not add any icons of its own.</string>

    <!-- Theme settings -->
    <bool name="config_iconpack">true</bool>
    <bool name="config_apexskin">false</bool>

    <!-- The feature graphic will appear above the theme description -->
    <string name="theme_feature">feature</string>

    <!-- Each theme may have up to 5 preview images -->
    <string name="theme_preview1">preview1</string>
    <string name="theme_preview2">preview2</string>
    <string name="theme_preview3">preview3</string>
    <string name="theme_preview4">preview4</string>

    <!--ADW Strings-->
    <string name="hello">@string/theme_title</string>

    <!-- App Strings -->
    <string name="activity_text_string">Welcome to Lighten Icon Pack. Select your launcher below to apply this theme.</string>

    <string name="dialog_message">%1$s is not installed. Install launcher?</string>
    <string name="dialog_install">Install</string>
    <string name="dialog_cancel">Cancel</string>
</resources>
